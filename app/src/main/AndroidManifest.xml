<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />
    <uses-permission android:name="android.permission.CAMERA" />
    <application
        android:hardwareAccelerated="false"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CV2Project"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CV2Project">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AnnouncementActivity" />
        <activity android:name=".AttendanceActivity" />
        <activity android:name=".NoticeActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".PerformanceReportActivity" />
        <activity android:name=".PickupServiceActivity" />
        <activity android:name=".PoseAnalysisActivity" />
        <activity android:name=".ScheduleActivity" />
        <activity android:name=".AddAnnouncementActivity" />
        <activity android:name=".DetailAnnouncementActivity" />
        <activity android:name=".StudentManagementActivity" />
        <activity android:name=".AddNoticeActivity" />
        <activity android:name=".DetailNoticeActivity" />
    </application>

</manifest>